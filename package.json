{
  "name": "@forthright/vile-bundler-audit",
  "version": "0.2.2",
  "description": "A vile plugin for bundler-audit",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "lib_ruby",
    "src",
    "Gemfile",
    "Gemfile.lock",
    "README.md"
  ],
  "scripts": {
    "bundle": "bin/bundle-rb-gems",
    "compile": "babel -d lib -m common src",
    "compile-dev": "bin/compile-dev",
    "dev": "bin/dev",
    "postinstall": "npm run bundle",
    "test": "bin/test",
    "test-all": "npm test && npm run test-ruby",
    "test-cov": "bin/test-cov",
    "test-ruby": "rspec test/*.rb",
    "vile": "vile -cpd --nodecorations"
  },
  "keywords": [
    "vile",
    "bundle",
    "bundler",
    "bundle-audit",
    "gem",
    "ruby"
  ],
  "dependencies": {
    "@forthright/vile": "~0.5.2",
    "bluebird": "~3.1.1",
    "lodash": "~3.10.1"
  },
  "devDependencies": {
    "@forthright/vile-coffeelint": "0.0.4",
    "@forthright/vile-comment": "0.0.4",
    "@forthright/vile-coverage": "^0.1.6",
    "@forthright/vile-depcheck": "0.0.3",
    "@forthright/vile-eclint": "0.0.2",
    "@forthright/vile-eslint": "0.0.3",
    "@forthright/vile-fs-lint": "^0.1.1",
    "@forthright/vile-git": "^0.1.0",
    "@forthright/vile-jshint": "0.0.2",
    "@forthright/vile-language": "0.0.4",
    "@forthright/vile-license": "0.0.3",
    "@forthright/vile-ncu": "0.0.4",
    "@forthright/vile-newline": "0.0.2",
    "@forthright/vile-nsp": "^0.1.0",
    "@forthright/vile-semver": "0.0.2",
    "@forthright/vile-whitespace": "0.0.3",
    "babel": "~5.8.34",
    "chai": "~3.4.1",
    "chai-as-promised": "^5.2.0",
    "coffee-script": "~1.10.0",
    "istanbul": "^0.4.1",
    "mimus": "~0.1.2",
    "mocha": "~2.3.4",
    "release-it": "~2.3.1",
    "sinon": "~1.17.2",
    "sinon-chai": "~2.8.0"
  }
}
