{
  "name": "@forthright/vile-bundler-audit",
  "version": "0.3.1",
  "description": "A vile plugin for bundler-audit",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "lib_ruby",
    "src",
    "Gemfile",
    "Gemfile.lock",
    "README.md"
  ],
  "scripts": {
    "bundle": "bin/bundle-rb-gems",
    "compile": "babel -d lib -m common src",
    "compile-dev": "bin/compile-dev",
    "dev": "bin/dev",
    "postinstall": "npm run bundle",
    "test": "bin/test",
    "test-all": "npm test && npm run test-ruby",
    "test-cov": "bin/test-cov",
    "test-ruby": "rspec test/*.rb",
    "vile": "vile p -usi --nodecorations",
    "release-it": "release-it"
  },
  "keywords": [
    "vile",
    "bundle",
    "bundler",
    "bundle-audit",
    "gem",
    "ruby"
  ],
  "dependencies": {
    "bluebird": "~3.4.6",
    "lodash": "^4.17.1"
  },
  "devDependencies": {
    "@forthright/vile": ">=0.11.0",
    "@forthright/vile-coffeelint": ">=0.1.13",
    "@forthright/vile-comment": ">=0.1.16",
    "@forthright/vile-coverage": ">=0.2.13",
    "@forthright/vile-depcheck": ">=0.1.11",
    "@forthright/vile-eclint": ">=0.1.9",
    "@forthright/vile-eslint": ">=0.1.12",
    "@forthright/vile-fs-lint": ">=0.3.8",
    "@forthright/vile-git": "^0.5.0",
    "@forthright/vile-jshint": ">=0.2.11",
    "@forthright/vile-language": ">=0.1.11",
    "@forthright/vile-ncu": ">=0.2.0",
    "@forthright/vile-newline": ">=0.1.11",
    "@forthright/vile-nsp": ">=0.2.11",
    "@forthright/vile-semver": ">=0.1.10",
    "@forthright/vile-stat": ">=0.0.11",
    "@forthright/vile-whitespace": ">=0.1.10",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "~3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-script": "~1.11.1",
    "istanbul": "^0.4.5",
    "mimus": "~0.2.3",
    "mocha": "~3.1.2",
    "npm-check-updates": ">=2.8.6",
    "release-it": "^2.5.1",
    "sinon": "~1.17.6",
    "sinon-chai": "~2.8.0"
  },
  "peerDependencies": {
    "@forthright/vile": ">=0.11.0"
  }
}
