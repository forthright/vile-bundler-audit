{
  "name": "@forthright/vile-bundler-audit",
  "version": "0.2.12",
  "description": "A vile plugin for bundler-audit",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "lib_ruby",
    "src",
    "Gemfile",
    "Gemfile.lock",
    "README.md"
  ],
  "scripts": {
    "bundle": "bin/bundle-rb-gems",
    "compile": "babel -d lib -m common src",
    "compile-dev": "bin/compile-dev",
    "dev": "bin/dev",
    "postinstall": "npm run bundle",
    "test": "bin/test",
    "test-all": "npm test && npm run test-ruby",
    "test-cov": "bin/test-cov",
    "test-ruby": "rspec test/*.rb",
    "vile": "vile -cpisd --nodecorations"
  },
  "keywords": [
    "vile",
    "bundle",
    "bundler",
    "bundle-audit",
    "gem",
    "ruby"
  ],
  "dependencies": {
    "bluebird": "~3.3.4",
    "lodash": "^4.9.0"
  },
  "devDependencies": {
    "@forthright/vile": ">= 0.7.0",
    "@forthright/vile-coffeelint": ">=0.1.9",
    "@forthright/vile-comment": ">=0.1.13",
    "@forthright/vile-coverage": ">=0.2.9",
    "@forthright/vile-depcheck": ">=0.1.8",
    "@forthright/vile-eclint": ">=0.1.6",
    "@forthright/vile-eslint": ">=0.1.7",
    "@forthright/vile-fs-lint": ">=0.3.5",
    "@forthright/vile-git": "^0.3.0",
    "@forthright/vile-jshint": ">=0.2.7",
    "@forthright/vile-language": ">=0.1.8",
    "@forthright/vile-ncu": ">=0.1.14",
    "@forthright/vile-newline": ">=0.1.8",
    "@forthright/vile-nsp": ">=0.2.8",
    "@forthright/vile-semver": ">=0.1.7",
    "@forthright/vile-stat": ">=0.0.8",
    "@forthright/vile-whitespace": ">=0.1.7",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "~3.5.0",
    "chai-as-promised": "^5.3.0",
    "coffee-script": "~1.10.0",
    "istanbul": "^0.4.3",
    "mimus": "~0.2.1",
    "mocha": "~2.4.5",
    "npm-check-updates": "^2.6.2",
    "release-it": "^2.3.1",
    "sinon": "~1.17.3",
    "sinon-chai": "~2.8.0"
  },
  "peerDependencies": {
    "@forthright/vile": ">= 0.7.0"
  }
}
